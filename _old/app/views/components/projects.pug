block projects
  .row
    each project in projects
      //- .card.bg-light.shadow.mb-4
      .row.g-0
        .col-md-4.pt-3.text-center
          img.rounded-start.project-thumbnail(src=project.image_link alt='...')
        .col-md-8.p-2
          .card.bg-light.shadow.mb-4
            .card-body
              h5.card-title=project.name
              p.card-text=project.desc
              p.card-text.small Built with...
                br
                each technology in project.technologies
                  small.technology-item=technology
              //- div.mt-2
              .row.mt-2
                .col-sm-12.text-end
                  a.me-2.btn.btn-sm.btn-outline-dark(data-toggle="tooltip" data-placement="bottom" target="_blank" title="View source code on GitHub" href=project.github_link)
                    span View Source Code 
                      i.fab.fa-github
                  if project.demo_link
                    a.btn.btn-sm.btn-outline-success(data-toggle="tooltip" data-placement="bottom" target="_blank" title="Deployed to Heroku" href=project.demo_link)
                      span View Demo 
                        i.fas.fa-cloud-upload
